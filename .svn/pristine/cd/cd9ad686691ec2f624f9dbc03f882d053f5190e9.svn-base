.ob-proceed-button {
    padding: 0px 30px !important;
    color: #716dff;
    border: 1px solid #716dff !important;
    background: #fff;
    border-radius: 5px;
    height: 40px;
    margin-right: 10px !important;
}
.ob-cancel-button {
    padding: 0px 30px !important;
    margin-left:10px;
    color: #999;
    background: #ededed;
    border-radius: 5px;
}
.fund-transfer-content{
    background: #fff;
    padding: 20px 30px;
}
#menu-tab {
    padding: 10px 0;
    

    ul {
        list-style-type: none;
        display: flex;
        margin-bottom: 0;
        padding: 0;
        
        li {
            width: 20%;
            margin-right: 1%;
            box-sizing: border-box;
            padding: 10px 0;
            text-align: center;
        }
        .active {
            background-color: #15b499;
            color: #fff;
            padding: 10px; 
            
        }
    }
}