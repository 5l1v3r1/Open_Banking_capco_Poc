                <fieldset>
                    <legend>SWIFT Code</legend>
                    <input type="text" size="30">
                </fieldset>
            </div>
        </form>
        <mat-divider></mat-divider>

        <button mat-button class="ob-proceed-button">
            Proceed
        </button> 


</div>